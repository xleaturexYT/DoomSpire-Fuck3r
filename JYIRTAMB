-- Scripts:

local function IEKLYPB_fake_script() -- Credits.LocalScript 
	local script = Instance.new('LocalScript', Credits)

	local frame = script.Parent.Parent.Credits -- change “Test” to the name of the frane
	local open = false
	
	script.Parent.MouseButton1Click:Connect(function()
		if frame.Visible == false then
			frame.Visible = true
		end
	end)
	
end
coroutine.wrap(IEKLYPB_fake_script)()
local function JYOI_fake_script() -- Top1.Top1 Script 
	local script = Instance.new('LocalScript', Top1)

	local Top1 = script.Parent
	
	Top1.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-105.706, 129.167, -2.69081)
	end)
end
coroutine.wrap(JYOI_fake_script)()
