coroutine.wrap(YSTGUJ_fake_script)()
local function QVUFU_fake_script() -- Functions_2.LocalScript 
	local script = Instance.new('LocalScript', Functions_2)

	script.Parent.MouseButton1Down:connect(function()
		if script.Parent.Parent.FunctionFrameLol.Visible == false then
			script.Parent.Parent.FunctionFrameLol.Visible = true
			script.Parent.Text = "Functions"
		else
			script.Parent.Parent.FunctionFrameLol.Visible = false
			script.Parent.Text = "Functions"
		end
	end)
end
coroutine.wrap(QVUFU_fake_script)()
local function NYLX_fake_script() -- RGBFrame.LocalScript 
	local script = Instance.new('LocalScript', RGBFrame)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.01)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
	
		counter = counter + 0.01
	end
end
coroutine.wrap(NYLX_fake_script)()
